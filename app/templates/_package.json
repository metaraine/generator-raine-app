{
  "name": "<%=props.project%>",
  "version": "0.0.0",
  "description": "<%=props.description%>",
  "license": "ISC",
  "repository": "<%=username%>/<%=props.project%>",
  "author": {
    "name": "<%=pkg.author.name%>",
    "email": "<%=pkg.author.email%>",
    "url": "<%=pkg.author.url%>"
  },
  "engines": {
    "node": "0.10.x"
  },
  "scripts": {
  	"start": "node app/",
    "nodemon": "nodemon -w src app/",
    "test": "mocha --recursive --compilers coffee:coffee-script/register",
    "browser": "browserify -s $npm_package_name -o browser.js .",
    "pingheroku": "heroku info -s | grep web_url | cut -d= -f2 | xargs -I{} curl {} -w '%{http_code}' -so /dev/null"
  },
  "dependencies": {
    "express": "~4",
    "jade": "~1",
    "body-parser": "~1",
    "newrelic": "~1"
  },
  <%if(props.keywords.length) {
    %>"keywords": <%=props.prettyKeywords%>,
  <%}%>"devDependencies": {
    "browserify": "~6",
    "chai": "^1.10.0",
    "coffee-script": "~1",
    "event-stream": "^3.2.2",
    "gulp": "~3",
    "gulp-autoprefixer": "~2",
    "gulp-cache": "~0.2",
    "gulp-clean": "~0.3",
    "gulp-coffee": "~2",
    "gulp-concat": "~2",
    "gulp-embedlr": "~0.5",
    "gulp-filter": "^1",
    "gulp-imagemin": "~2",
    "gulp-inject": "~1",
    "gulp-jshint": "~1",
    "gulp-livereload": "~2",
    "gulp-minify-css": "~0.3",
    "gulp-ng-annotate": "^0.3",
    "gulp-notify": "^2.2.0",
    "gulp-open": "~0.3",
    "gulp-plumber": "^0.6.6",
    "gulp-rename": "~1",
    "gulp-sass": "1.2.4",
    "gulp-uglify": "~1",
    "gulp-util": "~3",
    "imagemin": "~0.1",
    "mocha": "^2.1.0",
    "nodemon": "^1.3.6",
    "run-sequence": "~1",
    "tiny-lr": "~0.1"
  }
}
